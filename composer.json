        "league/flysystem-aws-s3-v3": "^3.10",
        "php-coveralls/php-coveralls": "^2.4",
        "phpstan/phpstan": "^1.5.3",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.1.8",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.14.5",
        "squizlabs/php_codesniffer": "^3.5",
