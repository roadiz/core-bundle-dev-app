{
    "name": "roadiz/entity-generator",
    "description": "Roadiz sub-package which generate Doctrine entity classes for node-sources",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@roadiz.io",
            "homepage": "https://www.roadiz.io",
            "role": "Lead developer"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-json": "*",
        "nette/php-generator": "^4.1",
        "roadiz/nodetype-contracts": "^3.0.0",
        "symfony/string": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/options-resolver": "7.3.*"
    },
    "autoload": {
        "psr-4": {
            "RZ\\Roadiz\\EntityGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RZ\\Roadiz\\EntityGenerator\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "api-platform/doctrine-orm": "^4.1.18",
        "api-platform/symfony": "^4.1.18",
        "phpstan/phpdoc-parser": "<2",
        "phpstan/phpstan": "^1.5.3",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.5.x-dev",
            "dev-develop": "2.6.x-dev"
        }
    }
}
