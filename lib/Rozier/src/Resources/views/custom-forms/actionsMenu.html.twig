{% apply spaceless %}
    <nav data-action-menu class="actions-menu uk-navbar">
        {% if (is_granted('ROLE_ACCESS_CUSTOMFORMS_DELETE')) %}
            {% if item.id %}
                <a class="uk-button uk-button-small rz-no-ajax-link" href="{{ path('customFormsDuplicatePage', { id: item.id}) }}">
                    <span class="icon-container"><i class="uk-icon-rz-duplicate"></i></span>
                    <span class="label"><span class="label-text">{% trans %}customForm.duplicate{% endtrans %}</span></span>
                </a>
                <a class="uk-button uk-button-danger" href="{{ path('customFormsDeletePage', { id: item.id}) }}">
                    <span class="icon-container"><i class="uk-icon-rz-trash-o"></i></span>
                    <span class="label"><span class="label-text">{% trans %}delete{% endtrans %}</span></span>
                </a>
            {% endif %}
        {% endif %}
    </nav>
{% endapply %}
