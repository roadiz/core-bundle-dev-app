{
    "name": "roadiz/openid",
    "description": "Roadiz sub-package for handling OpenID authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@roadiz.io",
            "homepage": "https://www.roadiz.io",
            "role": "Lead developer"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "codercat/jwk-to-pem": "^1.0",
        "lcobucci/jwt": "^5.3",
        "psr/cache": ">=1.0.1",
        "roadiz/jwt": "2.4.x-dev",
        "roadiz/models": "2.4.x-dev",
        "roadiz/random": "2.4.x-dev",
        "symfony/event-dispatcher-contracts": "^2.4.0",
        "symfony/http-client": "6.4.*",
        "symfony/http-foundation": "6.4.*",
        "symfony/routing": "6.4.*",
        "symfony/security-core": "6.4.*",
        "symfony/security-csrf": "6.4.*",
        "symfony/security-http": "6.4.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.5.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "RZ\\Roadiz\\OpenId\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.3.x-dev",
            "dev-develop": "2.4.x-dev"
        }
    }
}
