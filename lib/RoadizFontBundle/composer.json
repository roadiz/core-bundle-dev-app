{
    "name": "roadiz/font-bundle",
    "description": "Manage and expose web fonts with Roadiz CMS",
    "license": "MIT",
    "keywords": [
        "cms",
        "backoffice",
        "roadiz",
        "rezo zero"
    ],
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@roadiz.io",
            "homepage": "https://www.roadiz.io",
            "role": "Lead developer"
        }
    ],
    "type": "symfony-bundle",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "doctrine/doctrine-bundle": "^2.8.1",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "~2.20.0",
        "league/flysystem": "^3.0",
        "roadiz/models": "2.6.*",
        "symfony/asset": "7.3.*",
        "symfony/cache": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/yaml": "7.3.*",
        "twig/extra-bundle": "^3.21",
        "twig/intl-extra": "^3.21",
        "twig/string-extra": "^3.21",
        "twig/twig": "^3.21"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.4",
        "phpstan/phpstan": "^1.5.3",
        "phpstan/phpdoc-parser": "<2",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpunit/phpunit": "^9.6",
        "roadiz/core-bundle": "2.6.*",
        "roadiz/documents": "2.6.*",
        "roadiz/entity-generator": "2.6.*",
        "roadiz/rozier-bundle": "2.6.*",
        "symfony/browser-kit": "7.3.*",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "7.3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false,
            "php-http/discovery": false
        }
    },
    "autoload": {
        "psr-4": {
            "RZ\\Roadiz\\FontBundle\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.6.x-dev",
            "dev-develop": "2.7.x-dev"
        }
    }
}
