{
    "name": "roadiz/entity-generator",
    "description": "Roadiz sub-package which generate Doctrine entity classes for node-sources",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@roadiz.io",
            "homepage": "https://www.roadiz.io",
            "role": "Lead developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "roadiz/nodetype-contracts": "~1.1.2",
        "symfony/string": "6.4.*",
        "symfony/yaml": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/options-resolver": "6.4.*"
    },
    "suggest": {
        "api-platform/core": "If you need to create ApiFilter annotation right into your entities"
    },
    "autoload": {
        "psr-4": {
            "RZ\\Roadiz\\EntityGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RZ\\Roadiz\\EntityGenerator\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^1.5.3",
        "phpunit/phpunit": "^9.5",
        "api-platform/core": "~3.2.14"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.3.x-dev",
            "dev-develop": "2.4.x-dev"
        }
    }
}
